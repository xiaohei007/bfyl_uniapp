    "xh-msg-dialog": "/components/xh-msg-dialog",
    "xh-notify": "/components/xh-notify",
    "xh-scan-code": "/components/xh-scan-code",
    "user-guide": "/components/userGuide",
    "repayments": "/components/repayments",
    "xh-navbar": "/components/xhNavbar/xh-navbar",
    "mescroll-body": "/components/mescroll-uni/mescroll-body",
    "xh-steps": "/components/xh-steps"
  },
  "sitemapLocation": "sitemap.json"
}