page::after {
  content: '';
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 2rpx;
  box-shadow: 0px -1px 7px 0px #c0c4cc;
  z-index: 9999;
}
.online-service .os-header {
  padding-left: 54.3rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-bottom: 54.3rpx;
}
.online-service .os-header-logo {
  width: 54.3rpx;
  height: 72.4rpx;
  margin-right: 9.05rpx;
}
.online-service .osh-tips {
  font-size: 28.96rpx;
  color: #333;
}
.online-service .osh-tips-small {
  font-size: 21.72rpx;
  color: #999;
}
.online-service .fruit {
  width: 144.8rpx;
  height: 144.8rpx;
}
.online-service .fruit-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-bottom: 18.1rpx;
}
.online-service .fruit-name {
  font-size: 25.34rpx;
  color: #333;
  margin-top: 9.05rpx;
}
.online-service .tips-msg {
  color: #F5A741;
  font-size: 28.96rpx;
  text-align: right;
  margin-right: 72.4rpx;
}
.online-service button {
  padding-left: 0;
  padding-right: 0;
  background-color: #FFFFFF;
  font-size: 0;
}
.online-service button:after {
  border: none;
}
.online-service .about-us {
  position: fixed;
  bottom: 5rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  font-size: 32rpx;
  padding-top: 10rpx;
  color: #409EFF;
}
.online-service .about-us::after {
  content: '';
  width: 100%;
  position: absolute;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  border-bottom: 1px solid #409EFF;
  bottom: -2rpx;
}

