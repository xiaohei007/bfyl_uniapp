<block a:if="{{isShow}}"><view class="user-guide"><view class="user-guide-black"></view><view class="user-guide-body userGuide"><image class="user-guide-close" src="../../../static/images/toast_close.png" mode="widthFix" data-event-opts="{{[['tap',[['close',['$event']]]]]}}" onTap="__e"></image><image class="user-guide-img" src="{{toastImg}}" mode="widthFix" data-event-opts="{{[['tap',[['imageClick',['$event']]]]]}}" onTap="__e"></image></view></view></block>