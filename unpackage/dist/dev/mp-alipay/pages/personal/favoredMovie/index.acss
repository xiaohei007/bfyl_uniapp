.favored-movie {
  position: fixed;
  top: 0;
  bottom: 0;
  width: 100%;
  background-color: #d7253b;
}
.favored-movie-title {
  width: 100%;
  display: block;
}
.favored-movie-center {
  position: absolute;
  width: 100%;
  box-sizing: border-box;
  padding: 0 90.5rpx 0 63.35rpx;
  top: 316.75rpx;
}
.favored-movie-bottom {
  position: absolute;
  bottom: 0;
  width: 100%;
}
@media screen and (min-height: 700px) {
.favored-movie-center {
    position: absolute;
    width: 100%;
    box-sizing: border-box;
    padding: 0 27.15rpx 0 0;
    top: 362rpx;
}
}

