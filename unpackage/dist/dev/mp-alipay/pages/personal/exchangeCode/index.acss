.exchange-code {
  position: absolute;
  width: 100%;
  top: 0;
  bottom: 0;
  padding-top: 119.46rpx;
}
.exchange-code .exchange-code-bg {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-image: -webkit-linear-gradient(top, #E71919, #A31015);
  background-image: linear-gradient(to bottom, #E71919, #A31015);
}
.exchange-code .exchange-code-logo01 {
  position: absolute;
  top: 0;
  left: 18.1rpx;
  right: 18.1rpx;
  height: 354.76rpx;
  width: auto;
}
.exchange-code .exchange-code-content {
  position: relative;
  width: 718.57rpx;
  height: 908.62rpx;
  margin: 0 auto;
}
.exchange-code .exchange_code-bg {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
}
.exchange-code .exchange-code-text {
  width: 479.65rpx;
  height: 195.48rpx;
  display: block;
  margin: 36.2rpx auto 0;
  position: relative;
}
.exchange-code .exchange-code-logo02 {
  height: 146.61rpx;
  width: 615.4rpx;
  display: block;
  margin: 0 auto;
  position: relative;
}
.exchange-code .qrcode {
  width: 362rpx;
  height: 362rpx;
  display: block;
  position: absolute;
  top: 416.3rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  background-color: #FFFFFF;
}
.exchange-code .tips-text {
  font-size: 25.34rpx;
  color: #000000;
  position: absolute;
  top: 814.5rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  white-space: nowrap;
}
.exchange-code .stress {
  font-size: 36.2rpx;
  color: #E02020;
}

