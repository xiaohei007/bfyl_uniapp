<view data-event-opts="{{[['tap',[['lookExchangeCode',['$event']]]]]}}" class="my-card-item" onTap="__e"><view class="my-card-item-header"><text class="shop-name">{{config.shop_name}}</text><text class="sign">{{title}}</text></view><view class="my-card-item-body"><view class="ieb-info"><view class="ieb-info-text effective-time">{{'有效期剩余：'+$root.f0+''}}</view><view class="ieb-info-text pot-num">{{'罐数：'+config.card_count+'罐'}}</view><view class="ieb-info-text paid">{{'已支付：¥'+config.pay_money+''}}</view></view><block a:if="{{config.status===0}}"><view class="look" hover-class="look-hover">查看兑换码</view></block></view></view>