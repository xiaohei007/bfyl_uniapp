<view class="my-card-bag"><home-tabs vue-id="5a8db47a-1" currTabs="{{currTabs}}" data-event-opts="{{[['^tabsChange',[['tabsChange']]]]}}" onTabsChange="__e" onVueInit="__l"></home-tabs><swiper class="page-list" current="{{currTabs}}" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" onChange="__e"><swiper-item class="{{((i===0)?'not-converted-list':'')}}" a:for="{{tabs}}" a:for-item="tab" a:for-index="i" a:key="i"><mescroll-item vue-id="{{'5a8db47a-2-'+i}}" isCheckAll="{{isCheckAll}}" currTabs="{{i}}" ref="__r" data-ref-in-for="mescrollItem" data-event-opts="{{[['^checkback',[['checkback']]],['^xhNotify',[['xhNotify']]]]}}" onCheckback="__e" onXhNotify="__e" onVueInit="__l"></mescroll-item></swiper-item></swiper><block a:if="{{currTabs==0}}"><view class="xh-submit-bar"><view class="xh-submit-bar-body"><xh-check class="service" vue-id="5a8db47a-3" label="{{'全选（已选: '+selectedNum+'）'}}" labelClass="submit-check-label" checked="{{isCheckAll}}" data-event-opts="{{[['^change',[['checkAll']]]]}}" onChange="__e" onVueInit="__l"></xh-check><block a:if="{{userInfo.mobile}}"><van-button vue-id="5a8db47a-4" disabled="{{selectedNum===0||isLoading}}" color="linear-gradient(to right, #E71919, #A31015)" block="{{true}}" data-event-opts="{{[['^click',[['exchange']]]]}}" data-com-type="wx" onClick="__e" onVueInit="__l">前往兑奖</van-button></block><block a:else><van-button vue-id="5a8db47a-5" disabled="{{selectedNum===0||isLoading}}" color="linear-gradient(to right, #E71919, #A31015)" open-type="getAuthorize" block="{{true}}" scope="phoneNumber" data-event-opts="{{[['^getAuthorize',[['$onAliGetAuthorize',['exchangeBefore','$event']]]],['^error',[['$onAliAuthError',['exchangeBefore','$event']]]]]}}" data-com-type="wx" onGetAuthorize="__e" onError="__e" onVueInit="__l">前往兑奖</van-button></block></view><view class="van-submit-bar__safe"></view></view></block><confirm-exchange vue-id="5a8db47a-6" ref="__r" data-ref="confirmExchange" onVueInit="__l"></confirm-exchange><xh-notify vue-id="5a8db47a-7" ref="__r" data-ref="xhNotify" onVueInit="__l"></xh-notify></view>