.login .wecome-login {
  color: #000000;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 197.29rpx;
}
.login .wecome-logo {
  width: 144.8rpx;
  height: 144.8rpx;
  margin-bottom: 36.2rpx;
  border-radius: 50%;
}
.login .wecome-msg {
  font-size: 36.2rpx;
  color: #000000;
  margin-bottom: 204.53rpx;
}
.login .wecome-msg-stress {
  color: #A61115;
}
.login .tips {
  text-align: center;
  color: #000000;
  font-size: 32.58rpx;
  margin-bottom: 14.48rpx;
}
.login .login-btn, .login .cancel-login-btn {
  margin-left: 66.97rpx;
  margin-right: 66.97rpx;
}
.login .cancel-login-btn {
  margin-top: 15rpx;
}
.login .van-button--default {
  color: #999;
}
.login .agreement {
  margin-top: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 32.58rpx;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.login .agreement-read {
  margin-left: 10rpx;
}
.login .agreement-name {
  color: #A61115;
}

