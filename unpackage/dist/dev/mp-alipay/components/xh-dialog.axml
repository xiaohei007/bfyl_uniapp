<block a:if="{{isShow}}"><view class="xh-old-dialog"><view class="{{(('xh-old-dialog-body')+' '+animName)}}"><image class="xh-old-dialog-header" src="../static/images/xh-dialog-header.png"></image><view class="xh-old-dialog-arc"><image class="xh-old-dialog-icons" src="../static/images/volume-icon.png"></image></view><slot></slot><view class="xh-old-dialog-bg"></view><image class="close" src="../static/images/toast_close.png" data-event-opts="{{[['tap',[['close',['$event']]]]]}}" onTap="__e"></image></view><view data-event-opts="{{[['touchMove',[['',['$event']]]]]}}" class="xh-old-dialog-black" catchTouchMove="__e"></view></view></block>