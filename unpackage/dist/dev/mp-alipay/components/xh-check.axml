<view data-event-opts="{{[['tap',[['change',['$event']]]]]}}" class="{{(('xh-check')+' '+((disabled)?'xh-check-disabled':''))}}" onTap="__e"><view class="{{(('iconfont xh-check-icon')+' '+(checked?'icon-chenggong':'disabled-icon'))}}" style="{{'color:'+(primaryColor)+';'+('font-size:'+(iconSize+'px')+';')}}"></view><view class="{{(('xh-check-label')+' '+labelClass)}}" style="{{'color:'+(labelColor||primaryColor)+';'}}">{{label}}</view></view>