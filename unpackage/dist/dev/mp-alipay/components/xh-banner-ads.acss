
.adContainer {
	margin: 0 25rpx;
	border-radius: 5px;
	position: relative;
	overflow: hidden;
	z-index: 1;
	-webkit-transform: translate3d(0, 0, 0);
	        transform: translate3d(0, 0, 0);
	box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
}
.adContainer-hover{
	box-shadow: 0 8px 16px 0 rgba(0, 0, 0, 0.2);
}

