<view data-event-opts="{{[['tap',[['$emit',['imageClick','$0'],['link']]]]]}}" class="{{(('easy-loadimage')+' '+imageClass)}}" onTap="__e"><image class="{{((('origin-img')+' '+((!openTransition)?'no-transition':''))+' '+((showTransition&&openTransition)?'show-transition':''))}}" src="{{imageSrc}}" mode="{{mode}}" data-event-opts="{{[['load',[['handleImgLoad',['$event']]]],['error',[['handleImgError',['$event']]]]]}}" onLoad="__e" onError="__e"></image><block a:if="{{isLoadError}}"><view class="loadfail-img"></view></block><block a:if="{{loadImg}}"><view class="{{(('loading-img')+' '+loadingMode)}}"></view></block></view>