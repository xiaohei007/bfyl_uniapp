<cover-view class="xh-text-dialog" style="{{'display:'+(isShow?'block':'none')+';'}}"><cover-view class="xh-text-dialog-body fadeInDownBig"><cover-image class="xh-text-dialog-header" src="../static/images/xh-dialog-header.png"></cover-image><cover-view class="xh-text-dialog-arc"><cover-image class="xh-text-dialog-line" src="../static/images/dialog-line.png"></cover-image><cover-image class="xh-text-dialog-icons" src="../static/images/volume-icon.png"></cover-image></cover-view><cover-view class="xh-dialog-content"><cover-view class="xh-msg-dialog"><cover-view class="msg">{{msg}}</cover-view><block a:if="{{msgSmall}}"><cover-view class="msg-small">{{''+msgSmall+''}}</cover-view></block><cover-view data-event-opts="{{[['tap',[['callBack',['$event']]]]]}}" class="xh-toast-button go-niuniu" onTap="__e">{{buttonText}}</cover-view></cover-view></cover-view><cover-view class="xh-text-dialog-bg"><cover-image class="dialog-bg" src="../static/images/dialog-bg.png"></cover-image></cover-view></cover-view><cover-image class="close fadeInOpacity" style="{{'display:'+(isShow?'block':'none')+';'}}" src="../static/images/toast_close.png" data-event-opts="{{[['tap',[['close',['$event']]]]]}}" onTap="__e"></cover-image><cover-view data-event-opts="{{[['touchMove',[['',['$event']]]]]}}" class="xh-text-dialog-black" catchTouchMove="__e"></cover-view></cover-view>