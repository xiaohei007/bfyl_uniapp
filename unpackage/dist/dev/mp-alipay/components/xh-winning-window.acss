.winning-Window {
  position: relative;
  z-index: 1;
  padding: 0 68.78rpx;
}
.winning-Window .ww-title {
  padding-top: 18.1rpx;
  font-size: 57.92rpx;
  color: #F6E2B3;
  text-align: center;
}
.winning-Window .ww-msg {
  font-size: 27.15rpx;
  color: #fff;
  text-align: center;
  margin-top: 9.05rpx;
  margin-bottom: 18.1rpx;
}
.winning-Window .to-convert-right {
  height: 222.63rpx;
  padding-left: 72.4rpx;
  position: relative;
}
.winning-Window .to-convert-right-bg {
  position: absolute;
  bottom: 0;
  top: 0;
  left: 0;
  right: 0;
  z-index: -1;
}
.winning-Window .dui {
  position: absolute;
  width: 57.92rpx;
  height: 61.54rpx;
  right: 92.31rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.winning-Window .xh-toast-button {
  margin: 0 27.15rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.winning-Window .deposit {
  color: #F6E2B3;
  background-color: #680508;
}
.winning-Window .tcr-top-text {
  font-size: 36.2rpx;
  color: #FEEEB3;
  margin-top: 70rpx;
}
.winning-Window .tcr-bottom {
  font-size: 23.53rpx;
  margin-top: 10rpx;
  color: #F6E2B3;
}
.winning-Window .change-now {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-bottom: 36.2rpx;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}

