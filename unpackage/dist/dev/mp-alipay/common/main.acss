/*每个页面公共css */
page {
  font-family: PingFangSC, PingFangSC-Medium;
}
@font-face {
  font-family: 'iconfont';
  src: url("data:application/font-woff;charset=utf-8;base64,") format("truetype");
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-fabulous:before {
  content: "\e673";
  color: #F8AF1D;
}
.icon-fabulous-default:before {
  content: "\e63a";
}
.icon-browse-eye:before {
  content: "\e8cd";
  font-size: 20px;
}
.icon-chenggong:before {
  content: "\e867";
  font-size: 20px;
}
/*弹窗按钮样式*/
.xh-toast-button {
  height: 81.45rpx;
  background: #f6e2b3;
  border-radius: 999px;
  margin: 0 63.35rpx 0;
  font-size: 30.77rpx;
  color: #D62019;
  font-weight: bold;
  line-height: 81.45rpx;
  text-align: center;
}
/* 隐藏滚动条 */
::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
  color: transparent;
}
.xh-text-dialog {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 9999;
}
.xh-text-dialog .xh-text-dialog-body {
  position: fixed;
  top: 50%;
  left: 27.15rpx;
  right: 27.15rpx;
  z-index: 1;
}
.xh-text-dialog .xh-text-dialog-header {
  height: 253.4rpx;
  width: 70%;
  position: relative;
  z-index: 1;
  display: block;
  margin: 0 auto 108.6rpx;
}
.xh-text-dialog .xh-text-dialog-arc {
  position: absolute;
  height: 181rpx;
  top: 249.78rpx;
  left: 38.01rpx;
  right: 38.01rpx;
}
.xh-text-dialog .xh-text-dialog-icons {
  width: 81.45rpx;
  height: 86.88rpx;
  position: absolute;
  bottom: 81.45rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.xh-text-dialog .xh-text-dialog-line {
  height: 30rpx;
  margin-top: 30rpx;
}
.xh-text-dialog .xh-text-dialog-bg {
  position: absolute;
  left: 38.01rpx;
  right: 38.01rpx;
  bottom: 0;
  top: 190.05rpx;
  border-radius: 20px;
  z-index: -1;
  border: 3px solid #FFCB3E;
}
.xh-text-dialog .dialog-bg {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
}
.xh-text-dialog .xh-text-dialog-black {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  background-color: rgba(0, 0, 0, 0.4);
}
.xh-text-dialog .close {
  width: 72.4rpx;
  height: 72.4rpx;
  position: fixed;
  bottom: 12%;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  z-index: 1;
}
@-webkit-keyframes fadeInOpacity {
0% {
    -webkit-transform: translateY(2000px);
    transform: translateY(2000px);
}
to {
    -webkit-transform: translateX(-50%) translateY(0);
    transform: translateX(-50%) translateY(0);
}
}
@keyframes fadeInOpacity {
0% {
    -webkit-transform: translateY(2000px);
    transform: translateY(2000px);
}
to {
    -webkit-transform: translateX(-50%) translateY(0);
    transform: translateX(-50%) translateY(0);
}
}
.fadeInOpacity {
  -webkit-animation-name: fadeInOpacity;
  animation-name: fadeInOpacity;
  -webkit-animation-duration: 1s;
  -webkit-animation-fill-mode: both;
  animation-duration: 1s;
  animation-fill-mode: both;
}
@-webkit-keyframes fadeInDownBig {
0% {
    -webkit-transform: translateY(-2000px);
    opacity: 0;
    transform: translateY(-2000px);
}
to {
    -webkit-transform: translateY(-60%);
    opacity: 1;
    transform: translateY(-60%);
}
}
@keyframes fadeInDownBig {
0% {
    -webkit-transform: translateY(-2000px);
    opacity: 0;
    transform: translateY(-2000px);
}
to {
    -webkit-transform: translateY(-60%);
    opacity: 1;
    transform: translateY(-60%);
}
}
.fadeInDownBig {
  -webkit-animation-name: fadeInDownBig;
  animation-name: fadeInDownBig;
  -webkit-animation-duration: 1s;
  -webkit-animation-fill-mode: both;
  animation-duration: 1s;
  animation-fill-mode: both;
}
/*自定义弹窗*/
.xhDialog {
  -webkit-animation-name: xhDialog;
  animation-name: xhDialog;
  -webkit-animation-duration: 0.5s;
  -webkit-animation-fill-mode: both;
  animation-duration: 0.5s;
  animation-fill-mode: both;
}
@-webkit-keyframes xhDialog {
0% {
    -webkit-transform: translateY(-2000px);
    opacity: 0;
    transform: translateY(-2000px);
}
to {
    -webkit-transform: translateY(-60%);
    opacity: 1;
    transform: translateY(-60%);
}
}

