.login .login-icon {
  width: 260rpx;
  height: 240rpx;
  display: block;
  margin: 120rpx auto 50rpx;
}
.login .login-tips-title {
  font-size: 36rpx;
  color: #D33A29;
  text-align: center;
}
.login .login-tips-text {
  font-size: 28rpx;
  color: #666666;
  text-align: center;
  margin-bottom: 72rpx;
}
.login .login-btn, .login .cancel-login-btn {
  margin: 0 60rpx;
}
.login .cancel-login-btn {
  margin-top: 15rpx;
}
.login .van-button--default {
  color: #999;
}
.login .agreement {
  margin-top: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 25rpx;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.login .agreement-read {
  margin-left: 10rpx;
}
.login .agreement-name {
  color: #A61115;
  padding: 10rpx 0;
}
.login .login-warn {
  margin-top: 130rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 28rpx;
}
.login .login-warn > .login-warn-tips {
  color: #99abb4;
  margin-left: 10rpx;
}
.login .login-warn > .link-service {
  color: #5ea8f2;
  text-decoration: underline;
  margin-left: 5rpx;
  padding: 10rpx 0;
}

